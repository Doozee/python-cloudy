[COMMON]

### Git
git-user-full-name      = John Dole
git-user-email          = jdole@example.com

### Timezone & Locale
timezone                = Canada/Eastern
locale                  = en_US.UTF-8

### User & Group
admin-user              = fooman
admin-pass              = pass4fooman
admin-group             = admin

### Security
ssh-disable-root        = YES
ssh-enable-password     = YES
ssh-port                = 11011
ssh-key-path            = ~/.ssh/id_rsa.pub

### Server name
hostname                = example-generic

### Python
python-version          = 3.4

### Swap
#swap-size              = 512

### Shared github/bitbucket read-only keys
#shared-key-path         = ~/.ssh/shared/ssh
